<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.bookstore.domain.users.UsersMapperRepository">
    <select id="signIn" parameterType="com.project.bookstore.web.user.dto.UserSignInDto" resultType="com.project.bookstore.web.user.dto.UserInfoDto">
        SELECT *
        FROM users
        WHERE userID = #{userID} AND userPW=#{userPW};
    </select>
</mapper>
