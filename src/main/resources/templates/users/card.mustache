<form action="/users/addr" method="post">
    <div class="form-group">
        <label for="cardNum">카드번호</label>
        <input type="text" class="form-control" id="cardNum" placeholder="카드번호">
    </div>
    <div class="form-group">
        <label for="cardPeriod">유효기간</label>
        <input type="text" class="form-control" id="cardPeriod" placeholder="유효기간">
    </div>
    <div class="form-group">
        <label for="cardCVC">CVC번호</label>
        <input type="text" class="form-control" id="cardCVC" placeholder="CVC번호">
    </div>
    <div class="form-group">
        <label for="cardPW">결제비밀번호</label>
        <input type="text" class="form-control" id="cardPW" placeholder="결제비밀번호">
    </div>
    <div class="form-group">
        <label for="cardVal">카드종류</label>
        <input type="text" class="form-control" id="cardVal" placeholder="카드종류">
    </div>
</form>
